	xmlns="sap.m">
	<Toolbar visible="{viewModel>/userInCobrowse}">
		<ToolbarSpacer/>
		<MessageStrip id="msCobrowsingMessageStrip" showCloseButton="false" showIcon="true"
			text="{= ${i18n>messageCobrowsingForUser} + ' ' + ${viewModel>/cobrowseUserID} + ' ' + ${viewModel>/cobrowseUserDescription} }" type="Warning"></MessageStrip>
		<ToolbarSpacer/>
	</Toolbar>
</core:FragmentDefinition>