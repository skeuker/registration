<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="popContact" title="{viewModel>/popContactFormTitle}" class="sapUiContentPadding" placement="Bottom" contentWidth="650px">
		<content>
			<core:Fragment xmlns:core="sap.ui.core" fragmentName="capetown.gov.registration.fragment.reuse.Contact" type="XML"/>
		</content>
		<beginButton>
			<Button text="Cancel" press="onPressContactCancelButton"/>
		</beginButton>
		<endButton>
			<Button text="Confirm" type="Emphasized" press="onPressContactConfirmButton"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>